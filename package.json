{
  "name": "todo-extension",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --colors --passWithNoTests",
    "test:coverage": "jest --collectCoverage --no-cache",
    "start": "webpack-dev-server --open",
    "build:dev": "npm run clean && webpack -d --mode development",
    "build:prod": "npm run clean && webpack -p --mode production",
    "clean": "rimraf dist/",
    "lint": "eslint . --ext .ts --fix",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run test",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeonHyungJo/Todo-Extension.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SeonHyungJo/Todo-Extension/issues"
  },
  "homepage": "https://github.com/SeonHyungJo/Todo-Extension#readme",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.3",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "style-loader": "^1.1.2",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "graphql-request": "^1.8.2"
  }
}
