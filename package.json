    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "style-loader": "^1.1.2",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@types/redux-actions": "^2.6.1",
    "@types/redux-logger": "^3.0.7",
