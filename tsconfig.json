{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@githubApi/*": ["src/github-Api/*"],
      "@redux/*": ["src/redux/*"],
      "@model/*": ["src/model/*"],
      "@util/*": ["src/util/*"]
    }
  },
  "include": ["./src/**/*", "__test__/**/*"]
}
