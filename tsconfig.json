{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@githubApi/*": ["github-Api/*"],
      "@redux/*": ["redux/*"],
      "@model/*": ["model/*"],
      "@util/*": ["util/*"]
    }
  },
  "include": ["./src/**/*", "__test__/**/*"]
}
